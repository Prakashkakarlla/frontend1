{
    "buildCommand": "node node_modules/vite/bin/vite.js build",
    "redirects": [
        {
            "source": "/ads.txt",
            "destination": "https://srv.adstxtmanager.com/81044/jobfresh.in"
        }
    ],
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://api.jobfresh.in/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}