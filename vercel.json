{
    "buildCommand": "node node_modules/vite/bin/vite.js build",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://your-backend-api-url.com/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}